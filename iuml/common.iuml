skin rose

scale max 4096*4096

!$qcolor = "#indianred"


skinparam defaultfontcolor #brown
skinparam backgroundcolor transparent
skinparam arrowfontcolor #0F8C29
skinparam wrapwidth 180
skinparam titlefontcolor dodgerblue

skinparam note<<q>>backgroundcolor $qcolor

<style>
element {
  MaximumWidth 180
}
note {
  MaximumWidth 250
}
</style>


